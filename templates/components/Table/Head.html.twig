<thead class="text-center text-xs text-gray-700 uppercase whitespace-nowrap bg-gray-50 dark:bg-gray-700 dark:text-gray-400 {{ class|default('') }}">
	<tr>
		{% for col in columns|split(", ") %}
			<twig:Table:Header alternate="{{ alternate|default(false) and loop.index % 2 == 0 }}" class="px-6 py-3">
				{{ col }}
			</twig:Table:Header>
		{% endfor %}
	</tr>
</thead>
